<div align="center">
  <div>
    <h3 class="uk-heading-divider uk-card-title">Saved Characters</h3>

    <ul id="save-characters">
      {{#each savedCharacters as |char|}}
      <ul>
        <div class="uk-card uk-card-default uk-width-1-2@m">
          <div class="uk-card-header">
            <div class="uk-grid-small uk-flex-middle" uk-grid>
              <div class="uk-width-expand">
                <h2 id="char-name">Name: {{char.name}}</h2>
                <h2 id="char-culture" value="">Culture: {{char.culture}}</h2>
                <h2 id="char-aliases" value="">Aliases: {{char.aliases}}</h2>
                <h2 id="char-title" value="">Title: {{char.title}}</h2>
                <h2 id="char-house" value="">Houses: {{char.house}}</h2>
              </div>
            </div>
          </div>
        </div>

      </ul>

      {{/each}}

